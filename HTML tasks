  function remove() {
 var item = document.getElementById('Cola 1.5');
 item.parentNode.removeChild(item);
};

function replace (){
  var myEl = document.getElementById("Chocolate bar");
  var myEl2 = document.createElement("Canned Fish");
  myEl2.innerHTML = "replaced!";
  myEl.parentNode.replaceChild( myEl2);
}
# task 6
 function Accepter(){
      var button = document.getElementById('addTask');
      var taskWrap = document.getElementById('myTasks');
      function createLi(value){
        var li = document.createElement('li');
        li.innerText = value;
        taskWrap.appendChild(li);
      }
      function getTask(){
        var text = prompt("write your text:","");
        if(text.length > 0){
          createLi(text);
        }
      }
      button.addEventListener('click',getTask);
    } // ????
